    "@nrwl/linter": "^14.6.4",
    "@nrwl/node": "^14.6.4",
    "@nrwl/nx-cloud": "^14.6.2",
    "@nrwl/react": "^14.6.4",
    "@nrwl/web": "14.6.4",
    "@nrwl/workspace": "^14.6.4",
    "@swc/core": "^1.2.173",
    "@swc/jest": "0.2.20",
    "@testing-library/react": "13.3.0",
    "@types/express": "^4.17.14",
    "@types/jest": "^28.1.1",
    "@types/node": "^17.0.31",
    "@types/react": "18.0.18",
    "@types/react-dom": "18.0.6",
    "@types/supertest": "^2.0.12",
