{
  "name": "general-vms",
  "version": "1.0.0",
  "description": "General VMS example root directory",
  "engines": {
    "node": ">=14.14.0",
    "npm": ">=7"
  },
  "scripts": {
    "cdk": "cdk",
    "nx": "nx",
    "prepare": "node scripts/install-husky.js && npm run build:shared",
    "install:ci": "npm install --ignore-scripts",
    "setup:amplify": "node scripts/sync-amplify.js",
    "quality-checks": "npm run lint && npm run format:check && npm run test && npm run build",
    "dev": "nx run-many --target=serve",
    "lint": "nx run-many --target=lint",
    "test": "nx run-many --target=test",
    "build": "nx run-many --target=build",
    "commit-checks": "npm run format:check && npm run lint",
    "push-checks": "npm run quality-checks",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "copy-assets-for-cdk": "nx run-many --target=copy-assets-for-cdk",
    "clean": "rm -rf node_modules dist build cdk.out"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hack4impact-calpoly/general-vms.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hack4impact-calpoly/general-vms/issues"
  },
  "homepage": "https://github.com/hack4impact-calpoly/general-vms#readme",
